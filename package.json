{
  "name": "license-report-recursive",
  "version": "1.0.0",
  "description": "Wrapper around license-report to add recursion",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "test": "npx mocha -R spec",
    "lint": "eslint --report-unused-disable-directives -c .eslintrc.js index.js lib/**/*.js test/**/*.spec.js",
    "postinstall": "node scripts/postinstall.js",
    "init-changelog": "standard-version -i CHANGELOG.md --same-file --first-release",
    "release": "standard-version -i CHANGELOG.md --same-file"
  },
  "bin": {
    "license-report": "index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bepo65/license-report-recursive"
  },
  "author": "BePo65",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bepo65/license-report-recursive/issues"
  },
  "homepage": "https://github.com/bepo65/license-report-recursive",
  "dependencies": {
    "debug": "^4.3.4",
    "license-report": "^5.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.4",
    "@commitlint/config-conventional": "^16.2.4",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "eslint": "^8.14.0",
    "eslint-plugin-chai-expect": "^3.0.0",
    "eslint-plugin-mocha": "^10.0.4",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "husky": "^7.0.4",
    "mocha": "^9.2.2",
    "standard-version": "^9.3.2"
  },
  "engines": {
    "node": ">=14.0"
  }
}
